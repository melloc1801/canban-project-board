{
  "JWT": {
    "accessToken": {
      "expiresMinutes": 15,
      "secret": "HERE_SHOULD_BE_YOUR_SECRET_ACCESS_TOKEN_KEY"
    },
    "refreshToken": {
      "expiresDays": 30,
      "secret": "HERE_SHOULD_BE_YOUR_SECRET_REFRESH_TOKEN_KEY"
    }
  },
  "Database": {
    "connectionString": "Server=DB_HOST;Database=DB_NAME;User Id=DB_USERNAME;Password=DB_PASSWORD;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
